import statsmodels.api as sm

X = df["Study_Hours"]
y = df["GPA"]

X = sm.add_constant(X)  # Ø¥Ø¶Ø§ÙØ© Ø§Ù„Ø­Ø¯ Ø§Ù„Ø«Ø§Ø¨Øª
model = sm.OLS(y, X).fit()

print(model.summary())
