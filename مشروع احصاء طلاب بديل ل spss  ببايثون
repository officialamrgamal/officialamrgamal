from scipy.stats import f_oneway

anova_result = f_oneway(df[df["Group"] == "Experimental"]["GPA"],
                        df[df["Group"] == "Control"]["GPA"])
print(f"F-Statistic: {anova_result.statistic}, P-Value: {anova_result.pvalue}")
